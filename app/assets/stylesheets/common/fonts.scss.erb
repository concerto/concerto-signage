    src: url('/assets/maven_pro_regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('/assets/maven_pro_regular-webfont.woff') format('woff'),
         url('/assets/maven_pro_regular-webfont.ttf') format('truetype'),
         url('/assets/maven_pro_regular-webfont.svg#maven_proregular') format('svg');
    font-weight: normal;
    font-style: normal;
}