@font-face {
    font-family: 'maven_probold';
    src: url('/assets/maven_pro_bold-webfont.eot');
    src: url('/assets/maven_pro_bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('/assets/maven_pro_bold-webfont.woff') format('woff'),
         url('/assets/maven_pro_bold-webfont.ttf') format('truetype'),
         url('/assets/maven_pro_bold-webfont.svg#maven_probold') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'maven_proregular';
