b {
  font-family: "maven_probold";
  letter-spacing: -0.08em;
}
a {
  color: $linkColor;
  text-decoration: none;

  &:hover { color: $linkColorHover; }
}
h1, h2, h3, h4, h5, h6 {
  font-weight: 100;
  margin: 0;
}
h1 {
  font-size: 2.8em;
}
h2 {
  font-size: 2.4em;
}
h3 {
  font-size: 2em;
}
h4 {
  font-size: 1.5em;
}
h5 {
  font-size: 1.25em;
}

.center {
	text-align: center;
}

p {
  margin: 0;
}
